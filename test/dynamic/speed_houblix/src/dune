(rule
 (deps
  (file parser.mly))
 (mode (fallback))
 (targets sentence_1000.tokens)
 (action
  (run menhir parser.mly --provide-example sentence_1000.tokens)))

(rule
 (deps
  (file parser.mly))
 (mode (fallback))
 (targets sentence_100000.tokens)
 (action
  (run
   menhir
   parser.mly
   --provide-example
   sentence_100000.tokens
   --example-size
   100000)))

