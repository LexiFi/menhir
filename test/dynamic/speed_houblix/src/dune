(rule
 (alias speed)
 (deps
  code/houblix.exe
  table/houblix.exe
  old_code/houblix.exe
  sentence_1000.tokens
  sentence_200000.tokens
  sentence_200000_2.tokens
  sentence_200000_3.tokens
  ../speed.sh
  ../speed.ml)
 (action
  (chdir
   ..
   (run bash speed.sh))))
