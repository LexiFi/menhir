State 0:
main' -> . main [ # ]
-- On A shift to state 1
-- On main shift to state 4

State 1:
main -> A . B C [ # ]
-- On B shift to state 2

State 2:
main -> A B . C [ # ]
-- On C shift to state 3

State 3:
main -> A B C . [ # ]
-- On # reduce production main -> A B C

State 4:
main' -> main . [ # ]
-- On # accept main

