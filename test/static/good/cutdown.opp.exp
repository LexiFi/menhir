%{ %}
%start goal
%token ASSIGN
%token EOF
%token <string> INTEGER_LITERAL
%token L_BRACKET
%token NEW
%token R_BRACKET
%type <unit> goal
%%

goal:
  _1 = assignment _2 = EOF
    {                        ( )}

primary:
  _1 = INTEGER_LITERAL
    {                      ( )}
| _1 = array_access
    {                      ( )}

primary_not_name:
  _1 = array_access
    {                                             ( )}
| _1 = NEW _2 = L_BRACKET _3 = expression _4 = R_BRACKET
    {                                         ( )}

array_access:
  _1 = expression _2 = L_BRACKET _3 = primary_not_name _4 = R_BRACKET
    {                                                      ( )}

expression:
  _1 = primary
    {                         ( )}
| _1 = assignment
    {                         ( )}

assignment:
  _1 = left_hand_side _2 = ASSIGN _3 = expression
    {                                      ( )}

left_hand_side:
  _1 = primary_not_name
    {                       ( )}

%%
