%start main
%token A
%token B
%nonassoc block_is_finished
%nonassoc A
%type <unit> main
%%

list_block_:
  
    {    ( [] )} [@name nil_block]
| x = block xs = list_block_
    {    ( x :: xs )} [@name cons_block]

main:
  _1 = list_block_ _2 = B
    {                  ()}

block:
  _1 = items
    {                  ()}

items:
  _1 = item %prec block_is_finished
    {                  ()}
| _1 = item _2 = items
    {                  ()}

item:
  _1 = A
    {                  ()}

%%
