%start s
%token A
%token B
%token EOF
%type <int> s
%%

s:
  _2 = EOF
    {let _1 =     ( None ) in
                                                     ( 0 )}
| _1 = A x = B _3 = A _2 = EOF
    {let _1 =
  let x =
    let y =     ( x ) in
    let x =     ( None ) in
        ( (x, y) )
  in
      ( Some x )
in
                                                     ( 0 )}
| x = A _1 = A x_inlined1 = B _3 = A _2 = EOF
    {let _1 =
  let x =
    let y =
      let x = x_inlined1 in
          ( x )
    in
    let x =     ( Some x ) in
        ( (x, y) )
  in
      ( Some x )
in
                                                     ( 0 )}

%%
