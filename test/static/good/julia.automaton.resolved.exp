State 0:
main' -> . main [ # ]
-- On TReturn shift to state 1
-- On TIf shift to state 2
-- On statement shift to state 10
-- On main shift to state 12

State 1:
statement -> TReturn . [ TElse EOF ]
-- On TElse reduce production statement -> TReturn
-- On EOF reduce production statement -> TReturn

State 2:
statement -> TIf . TOPar expr TCPar statement [ TElse EOF ]
statement -> TIf . TOPar expr TCPar statement TElse statement [ TElse EOF ]
-- On TOPar shift to state 3

State 3:
statement -> TIf TOPar . expr TCPar statement [ TElse EOF ]
statement -> TIf TOPar . expr TCPar statement TElse statement [ TElse EOF ]
-- On TIdent shift to state 4
-- On expr shift to state 5

State 4:
expr -> TIdent . [ TCPar ]
-- On TCPar reduce production expr -> TIdent

State 5:
statement -> TIf TOPar expr . TCPar statement [ TElse EOF ]
statement -> TIf TOPar expr . TCPar statement TElse statement [ TElse EOF ]
-- On TCPar shift to state 6

State 6:
statement -> TIf TOPar expr TCPar . statement [ TElse EOF ]
statement -> TIf TOPar expr TCPar . statement TElse statement [ TElse EOF ]
-- On TReturn shift to state 1
-- On TIf shift to state 2
-- On statement shift to state 7

State 7:
statement -> TIf TOPar expr TCPar statement . [ TElse EOF ]
statement -> TIf TOPar expr TCPar statement . TElse statement [ TElse EOF ]
-- On TElse shift to state 8
-- On EOF reduce production statement -> TIf TOPar expr TCPar statement

State 8:
statement -> TIf TOPar expr TCPar statement TElse . statement [ TElse EOF ]
-- On TReturn shift to state 1
-- On TIf shift to state 2
-- On statement shift to state 9

State 9:
statement -> TIf TOPar expr TCPar statement TElse statement . [ TElse EOF ]
-- On TElse reduce production statement -> TIf TOPar expr TCPar statement TElse statement
-- On EOF reduce production statement -> TIf TOPar expr TCPar statement TElse statement

State 10:
main -> statement . EOF [ # ]
-- On EOF shift to state 11

State 11:
main -> statement EOF . [ # ]
-- On # reduce production main -> statement EOF

State 12:
main' -> main . [ # ]
-- On # accept main

