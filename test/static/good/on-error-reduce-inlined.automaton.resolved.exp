State 0:
main' -> . main [ # ]
-- On B shift to state 1
-- On A shift to state 2
-- On main shift to state 4

State 1:
main -> B . [ # ]
-- On # reduce production main -> B

State 2:
main -> A . B [ # ]
-- On B shift to state 3

State 3:
main -> A B . [ # ]
-- On # reduce production main -> A B

State 4:
main' -> main . [ # ]
-- On # accept main

