%start main
%token A
%token B
%token EOF
%type <unit> main
%%

aseq_A_B_:
  
    {                 ()}
| _1 = A _2 = aseq_B_A_
    {                 ()}

aseq_B_A_:
  
    {                 ()}
| _1 = B _2 = aseq_A_B_
    {                 ()}

main:
  _1 = aseq_A_B_ _2 = EOF
    {                 ()}

%%
