| _1 = long_ident _2 = COMMA _3 = long_ident_nonempty_list
    {                                              ( _1 :: _3 )}

function_descriptor:
  _1 = FUNCTION _2 = identifier
    {                        ( _2 )}
| _1 = CONSTRUCTOR
    {                        ( "*constructor*" )}
| _1 = RECEIVE
    {                        ( "*receive*" )}
| _1 = FALLBACK
    {                        ( "*fallback*" )}

return_list:
