    {let _1 =                                       ( _1 ) in
              ( Some (_1) )}
| _1 = ambiguous_type_name_or_expression
    {let _1 =                                       ( type_name_of_ambiguity _1 ) in
              ( Some (_1) )}
| _1 = STAR
    {              ( None )}

type_definition:
