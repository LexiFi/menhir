    {      ( StructDefinition (_2, _4) )}
| _1 = ENUM _2 = identifier _3 = LBRACE _4 = enum_value_list _5 = RBRACE
    {      ( EnumDefinition (_2, _4) )}

variable_declaration_semi_list:
  
    {                                                             ( [] )}
| _1 = variable_declaration _2 = SEMI _3 = variable_declaration_semi_list
    {                                                             ( _1 :: _3 )}

