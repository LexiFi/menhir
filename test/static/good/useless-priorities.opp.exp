%start main
%token EOF
%token <int> INT
%token PLUS
%token TIMES
%left PLUS
%left TIMES
%type <int> main
%%

expression:
  i = INT
    {                    ( i )}
| e = expression o = op f = expression
    {                                                                   ( o e f )}

op:
  _1 = PLUS
    {         ( ( + ) )}
| _1 = TIMES
    {                           ( ( * ) )}

main:
  e = expression _2 = EOF
    {                         ( e )}

%%
